#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/lib/runner/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/lib/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/lib/runner/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/lib/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules/pagefind/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/pagefind@1.1.0/node_modules:/Users/eeb4/Documents/GitHub/fv/fv-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pagefind@1.1.0/node_modules/pagefind/lib/runner/bin.cjs" "$@"
else
  exec node  "$basedir/../../../../../pagefind@1.1.0/node_modules/pagefind/lib/runner/bin.cjs" "$@"
fi
