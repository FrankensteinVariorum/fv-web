{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": [
      "ES2021", "dom", "ESNext"
    ],
    "types": [
      "astro/client"
    ],
    "jsx": "react",
  },
  "include": [
    "/src/*",
    "/src/**/*",
  ],

}